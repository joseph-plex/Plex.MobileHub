<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="ManagerSDKSoap" />
            </basicHttpBinding>
            <customBinding>
                <binding name="ManagerSDKSoap12">
                    <textMessageEncoding messageVersion="Soap12" />
                    <httpTransport />
                </binding>
            </customBinding>
        </bindings>
        <client>
            <endpoint address="http://pmh.plexxis.com/managersdk.asmx" binding="basicHttpBinding"
                bindingConfiguration="ManagerSDKSoap" contract="ManagementResources.ManagerSDKSoap"
                name="ManagerSDKSoap" />
            <endpoint address="http://pmh.plexxis.com/managersdk.asmx" binding="customBinding"
                bindingConfiguration="ManagerSDKSoap12" contract="ManagementResources.ManagerSDKSoap"
                name="ManagerSDKSoap12" />
        </client>
    </system.serviceModel>
</configuration>